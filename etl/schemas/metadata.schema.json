{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Metadata",
  "type": "object",
  "required": ["updated_at", "source_url", "total_venues", "window"],
  "properties": {
    "updated_at": { "type": "string" },
    "source_url": { "type": "string" },
    "total_venues": { "type": "integer" },
    "window": {
      "type": "object",
      "properties": {
        "start": { "type": "string" },
        "end": { "type": "string" }
      }
    }
  }
}
